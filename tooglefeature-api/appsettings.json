{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "MyFeatures": {
    "NewsAPI": true,
    "SportAPI": true,
    "WeatherAPI": false,
    "EconomyAPI": false,
    "MagazineAPI": {
      "EnabledFor": [
        {
          "Name": "Percentage",
          "Parameters": {
            "Value": 50
          }
        }
      ]
    },
    "WeekendNewsAPI": {
      "EnabledFor": [
        {
          "Name": "TimeWindow",
          "Parameters": {
            "Start": "Sat, 01 Oct 2022 00:00:00 GMT",
            "End": "Sun, 02 Oct 2022 23:59:59 GMT"
          }
        }
      ]
    }
  }
}
